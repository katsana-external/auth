{
  "name": "orchestra/auth",
  "description": "Auth Component for Orchestra Platform",
  "homepage": "http://orchestraplatform.com/docs/latest/components/auth/",
  "keywords": [
    "orchestra-platform",
    "orchestral",
    "laravel",
    "auth",
    "acl",
    "rbac",
    "permission"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mior Muhammad Zaki",
      "email": "crynobone@gmail.com",
      "homepage": "https://github.com/crynobone"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchestra\\Auth\\": "src/Auth/",
      "Orchestra\\Authorization\\": "src/Authorization/"
    },
    "classmap": [
      "database/migrations"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Orchestra\\Auth\\Tests\\": "tests/Auth/",
      "Orchestra\\Authorization\\Tests\\": "tests/Authorization/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/auth": "^9.0",
    "orchestra/contracts": "^7.0",
    "orchestra/memory": "^7.0",
    "orchestra/support-core": "^7.0"
  },
  "require-dev": {
    "orchestra/model": "7.x-dev",
    "orchestra/testbench": "^7.0"
  },
  "replace": {
    "orchestra/authorization": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "7.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/model"
    }
  ]
}
