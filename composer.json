{
	"name": "orchestra/auth",
	"description": "Orchestra Platform 2 Auth Component",
	"license": "MIT",
	"homepage": "http://orchestraplatform.com/docs/latest/components/auth/",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "auth", "acl", "rbac", "permission"],
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"classmap": [
			"src/migrations"
		],
		"psr-4": {
			"Orchestra\\Auth\\" : "src/Auth/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"illuminate/contracts": "4.3.*",
		"orchestra/memory": "2.3.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"patchwork/utf8": "1.1.*",
		"illuminate/auth": "4.3.*",
		"illuminate/database": "4.3.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/auth component outside of Orchestra Platform 2",
		"orchestra/model": "Allow using orchestra/auth component outside of Orchestra Platform 2",
		"orchestra/foundation": "Allow using orchestra/auth with Orchestra Platform 2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.3-dev"
		}
	},
	"minimum-stability": "dev"
}
